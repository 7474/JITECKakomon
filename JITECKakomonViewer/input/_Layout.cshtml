<!DOCTYPE html>
<html>
<head>
	<meta charset="utf-8">
	<meta name="viewport" content="width=device-width, initial-scale=1.0">
	@await RenderSectionAsync("Head", false)

	@{
		string title = @Document.ContainsKey(Keys.Title) ? $"{Document.GetString(Keys.Title)} - IPA過去問Bot" : "IPA過去問Bot";
	}
	<title>@title</title>
	<link href="https://cdn.jsdelivr.net/npm/bootstrap@5.0.2/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-EVSTQN3/azprG1Anm3QDgpJLIm9Nao0Yz1ztcQTwFspd3yD65VohhpuuCOmLASjC" crossorigin="anonymous">
	<script async src="https://platform.twitter.com/widgets.js" charset="utf-8"></script>
</head>
<body class="bg-light">
	<header class="d-flex flex-wrap justify-content-center py-3 mb-4 border-bottom">
		<a href="/" class="d-flex align-items-center mb-3 mb-md-0 me-md-auto text-dark text-decoration-none">
			<span">IPA過去問Bot</span>
		</a>
		@*
			<ul class="nav nav-pills">
			<li class="nav-item"><a href="#" class="nav-link active" aria-current="page">Home</a></li>
			<li class="nav-item"><a href="#" class="nav-link">Features</a></li>
			<li class="nav-item"><a href="#" class="nav-link">Pricing</a></li>
			<li class="nav-item"><a href="#" class="nav-link">FAQs</a></li>
			<li class="nav-item"><a href="#" class="nav-link">About</a></li>
			</ul>
		*@
	</header>
	<main class="container-fluid bg-body">
		@RenderBody()
	</main>
	<footer>
		<a href="https://twitter.com/ipa_kakomon">@@ipa_kakomon</a>
	</footer>
		@await RenderSectionAsync("Script", false)
</body>
</html>